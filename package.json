{
  "name": "risingstack-docker-graphql",
  "version": "1.0.0",
  "description": "Build CRA, RisingStack, Docker, and GraphQL setup from scratch",
  "main": "api/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thousight/RisingStack-Docker-GraphQL.git"
  },
  "keywords": [
    "risingstack",
    "docker",
    "graphql",
    "koa",
    "postgresql",
    "redis"
  ],
  "author": "thousight",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thousight/RisingStack-Docker-GraphQL/issues"
  },
  "homepage": "https://github.com/thousight/RisingStack-Docker-GraphQL#readme",
  "scripts": {
    "start": "node dist/server.js",
    "dev:web": "react-app-rewired start",
    "dev:api": "nodemon --watch ./api/ --exec babel-node ./api/server.js  --presets=env",
    "build": "react-app-rewired build && babel ./api -d dist && mv build dist/public",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "postinstall": "npm run build-css",
    "build-css": "lessc --relative-urls ./src/index.less ./src/index.css",
    "watch-css": "npm run build-css && lessc --relative-urls ./src/index.less ./src/index.css"
  },
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "joi": "^13.4.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-logger": "^3.2.0",
    "koa-redis": "^3.1.2",
    "koa-router": "^7.4.0",
    "less": "^3.8.0",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^1.1.4",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-bluebird": "^1.0.1",
    "babel-preset-env": "^1.7.0",
    "less-loader": "^4.1.0",
    "react-app-rewire-less": "^2.1.2",
    "react-app-rewired": "^1.5.2"
  }
}
